require 'rubygems'
require 'betabuilder'

BetaBuilder::Tasks.new do |config|
  # your Xcode target name
  config.target = "baby ketten"
  config.build_dir = :derived
  config.xcode4_archive_mode = true
  config.workspace_path = "Baby Ketten.xcodeproj/project.xcworkspace"
  config.scheme = "baby ketten"
  config.app_name = "baby ketten"

end
